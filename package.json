{
  "name": "fap-web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "concurrently \"velite dev\" \"next dev\"",
    "build": "velite build && next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "velite build && tsc --noEmit",
    "test:contract": "vitest run tests/contracts",
    "test:a11y": "vitest run tests/a11y",
    "test:e2e": "playwright test",
    "test:e2e:visual": "playwright test tests/e2e/visual --workers=1",
    "release:gate": "bash scripts/release_gate.sh",
    "postbuild": "next-sitemap && dotenv -e .env.production.local -- node scripts/upload-next-static-to-cos.mjs"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "cos-nodejs-sdk-v5": "^2.15.4",
    "lucide-react": "^0.563.0",
    "mime-types": "^3.0.2",
    "next": "16.1.2",
    "next-sitemap": "^4.2.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "velite": "^0.3.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.19.33",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "zod": "^4.3.6"
  }
}
