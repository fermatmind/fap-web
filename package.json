{
  "name": "fap-web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "concurrently \"velite dev\" \"next dev\"",
    "build": "velite build && next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "velite build && tsc --noEmit",
    "release:gate": "bash scripts/release_gate.sh",
    "postbuild": "next-sitemap && node scripts/upload-next-static-to-cos.mjs"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "cos-nodejs-sdk-v5": "^2.15.4",
    "lucide-react": "^0.563.0",
    "mime-types": "^3.0.2",
    "next": "16.1.2",
    "next-sitemap": "^4.2.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "velite": "^0.3.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.33",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "zod": "^4.3.6"
  }
}
